{
  "name": "CSPSkeleton",
  "version": "1.0.0",
  "description": "CSP Skeleton for JS with Channels",
  "main": "source/index.js",
  "scripts": {
    "devserver": "webpack-dev-server --hot --inline --colors --port 8088 --json --progress",
    "cleanDirWin": "del /q www\\* && for /d %x in (www\\*) do @rd /s /q \"%x\"",
    "cleanDirOSX": "rm -rf www/*",
    "dumpdev": "npm run cleanDirWin && webpack",
    "dumpprod": "npm run cleanDirWin && webpack --release --config webpack.config.prod.js"
  },
  "author": "lee.owen@MadeByAi.co",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "babel-polyfill": "^6.2.0",
    "cordova": "^5.3.3",
    "css-loader": "^0.20.1",
    "deep-extend": "^0.4.0",
    "exports-loader": "^0.6.2",
    "expose-loader": "^0.7.0",
    "file-loader": "^0.8.4",
    "html-loader": "^0.3.0",
    "html-webpack-plugin": "^1.6.2",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.3",
    "node-sass": "^3.3.3",
    "path": "^0.12.7",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "util": "^0.10.3",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "js-csp": "^0.5.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "immutable-js": "^0.3.1-6",
    "extract-text-webpack-plugin": "^0.8.2",
    "glob": "^5.0.15",
    "lodash": "^3.10.1",
    "normalize.css": "^3.0.3",
    "object-sizeof": "^1.0.6"
  }
}
